{
  "name": "demo-dapp-rainbowkit",
  "description": "Ethereum Demo Dapp + RainbowKit built on Sequence stack",
  "version": "0.1.0",
  "private": true,
  "homepage": "demo-dapp-rainbowkit",
  "scripts": {
    "dev": "BROWSER=none pnpm start",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "clear:vite:cache": "rm -rf node_modules/.vite/",
    "link-sequence": "../sequence.js/scripts/pnpm-link.sh link",
    "unlink-sequence": "../sequence.js/scripts/pnpm-link.sh unlink"
  },
  "dependencies": {
    "0xsequence": "^1.9.14",
    "@0xsequence/design-system": "^1.2.5",
    "@0xsequence/ethauth": "^0.8.1",
    "@0xsequence/rainbowkit-plugin": "^2.1.7",
    "@0xsequence/utils": "^1.9.14",
    "@rainbow-me/rainbowkit": "^1.0.6",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@vanilla-extract/css": "^1.14.2",
    "@wagmi/chains": "^0.1.14",
    "ethers": "^5.7.2",
    "framer-motion": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.4",
    "viem": "^1.3.0",
    "wagmi": "^1.3.9"
  },
  "devDependencies": {
    "@vanilla-extract/vite-plugin": "^4.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.2.8",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
